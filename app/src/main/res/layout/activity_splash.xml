<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B9F8B7"
    tools:context=".userinterface.splash.SplashActivity">

    <ImageView
        android:id="@+id/splashImage"
        android:layout_width="@dimen/splash_image_width"
        android:layout_height="@dimen/splash_image_height"
        android:layout_marginStart="@dimen/splash_image_margin_start"
        android:layout_marginTop="@dimen/splash_image_margin_top"
        android:layout_marginEnd="@dimen/splash_image_margin_end"
        android:layout_marginBottom="@dimen/splash_image_margin_bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/thoughtfocuslogoremovebg" />

</androidx.constraintlayout.widget.ConstraintLayout>